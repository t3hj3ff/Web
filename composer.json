{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.3",
        "ext-zip": "*",
        "cartalyst/sentinel": "^3.0",
        "doctrine/dbal": "2.10.2",
        "eluceo/ical": "0.16.*",
        "fideloper/proxy": "^4.0",
        "glaivepro/hidevara": "1.0.5",
        "gumlet/php-image-resize": "1.9.*",
        "johngrogg/ics-parser": "^2",
        "laravel/framework": "6.*",
        "laravel/tinker": "^1.0",
        "madnest/madzipper": "^1.0",
        "matthiasmullie/minify": "^1.3",
        "phpmailer/phpmailer": "^6.1",
        "symfony/translation": "4.3.8",
        "tedivm/jshrink": "~1.0",
        "tinify/tinify": "^1.5",
		"psr/container": "1.0.0",
        "ext-curl": "*",
      "ext-json": "*"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "veyvey.php",
            "gateways/paypal/vendor/autoload.php",
            "gateways/stripe/vendor/autoload.php",
            "plugins/facebook/vendor/autoload.php",
            "plugins/google/vendor/autoload.php",
            "plugins/google-captcha/vendor/autoload.php",
            "plugins/rachidlaasri/laravel-installer/vendor/autoload.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
